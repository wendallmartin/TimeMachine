<Window x:Class="TheTimeApp.Controls.PrevEmailWin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             WindowStartupLocation="CenterScreen"
             Height="80" Width="186">
    <Grid>
        <Button Content="Email" FontSize="16" Focusable="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin='3,0,0,0' Width="80" Height="40" Click="EmailBtn_Click"/>
        <Button Content="Preview" FontSize="16" Focusable="False" HorizontalAlignment="Right" VerticalAlignment="Center" Margin='0,0,3,0' Width="80" Height="40" Click="PrevBtn_Click"/>
    </Grid>
</Window>
